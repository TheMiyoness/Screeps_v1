var createHarvester = {
    run: function() {
	      
        var newName = 'Harvester' + Game.time;
        
        Game.spawns['Spawn1'].spawnCreep(
			[WORK,CARRY,MOVE],
			newName,
			{memory:
				{role: 'harvester'}
			}
		);
        
		console.log('Spawning new harvester: ' + newName);
	}
};
module.exports = createHarvester;
